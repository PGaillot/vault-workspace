            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Meta, StoryObj } from '@storybook/angular';\nimport { Component, Input } from '@angular/core';\nimport { HoverBorder } from './hover-border';\n\n@Component({\n  selector: 'hover-border-demo',\n  standalone: true,\n  imports: [HoverBorder],\n  styleUrls: ['../styles/index.scss'],\n  template: `\n    <div vaultHoverBorder\n      [style.display]=\"'inline-block'\"\n      [style.padding]=\"padding\"\n      [style.background]=\"background\"\n      [style.color]=\"textColor\"\n      [style.position]=\"'relative'\">\n      {{ text }}\n    </div>\n  `,\n})\nclass HoverBorderDirectiveDemo {\n  @Input() text = 'Survolez-moi pour voir la bordure';\n  @Input() padding = '1rem';\n  @Input() background = '#f0f0f0';\n  @Input() textColor = '#000000';\n}\n\n// --- Stories ---\n\nconst meta: Meta<HoverBorderDirectiveDemo> = {\n  title: 'Directives/HoverBorder',\n  component: HoverBorderDirectiveDemo,\n  argTypes: {\n    text: { control: 'text' },\n    padding: { control: 'text' },\n    background: { control: 'color' },\n    textColor: { control: 'color' },\n  },\n};\n\nexport default meta;\n\ntype Story = StoryObj<HoverBorderDirectiveDemo>;\n\nexport const Default: Story = {\n  args: {\n    text: 'Survolez-moi pour voir la bordure',\n    padding: '1rem',\n    background: '#f0f0f0',\n    textColor: '#000000',\n  },\n};\n\nexport const WithCustomColors: Story = {\n  args: {\n    ...Default.args,\n    text: 'Bordure colorée',\n    background: '#2dc92d',\n    textColor: '#ffffff',\n  },\n};\n\nexport const LargePadding: Story = {\n  args: {\n    ...Default.args,\n    text: 'Grande zone de padding',\n    padding: '2rem',\n    background: '#4a90e2',\n    textColor: '#ffffff',\n  },\n};\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": "@forward 'variables';\n@forward 'colors';\n@forward 'mixins';\n@forward 'hover-border-directive';",
                    "styleUrl": "../styles/index.scss"
                }
            ],
